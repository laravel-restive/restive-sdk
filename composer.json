{
  "name": "laravel-restive/restive-sdk",
  "type": "library",
  "license": "MIT",
  "description": "An API framework for Laravel",
  "keywords": ["rest", "api", "laravel"],
  "authors": [
    {
      "name": "Ian C Wilson"
    }
  ],
  "autoload": {
    "psr-4": {
      "RestiveSDK\\": "src/"
    }
  },
  "require": {
    "league/pipeline": "^1.0",
    "guzzlehttp/guzzle": "^6.3|^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "~5.0|~6.0",
    "phpunit/phpunit": "^8.0|^9.0",
    "codedungeon/phpunit-result-printer": "^0.19.12",
    "nunomaduro/collision": "^4.0",
    "mockery/mockery": "^1.1"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "tests": "phpunit -c phpunit.xml",
    "coverage": "phpunit -c phpunit-cc.xml"
  }
}
